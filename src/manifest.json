{
  "name": "微信群发助手",
  "manifest_version": 2,
  "version": "3.1.0",
  "icons": {
    "128": "icon.png"
  },
  "description": "群发信息给好友, 452125301.hzplay@gmail.com",
  "content_scripts": [
    {
      "matches": [
        "http://*.qq.com/*",
        "https://*.qq.com/*",
        "http://*.wechat.com/*",
        "https://*.wechat.com/*"
      ],
      "js": [
        "popup.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "app.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "微信群发助手",
    "default_popup": "popup.html"
  }
}

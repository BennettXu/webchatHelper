{
  "name": "微信群发助手",
  "manifest_version": 2,
  "version": "2.0",
  "icons": {
    "128": "icon.png"
  },
  "description": "就是给 好友 或者 公众号 或者 组 批量发信息",
  "content_scripts": [
    {
      "matches": [
        "http://*.qq.com/*",
        "https://*.qq.com/*"
      ],
      "js": [
        "popup.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "app.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "微信群发助手",
    "default_popup": "popup.html"
  }
}
